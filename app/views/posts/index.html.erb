	 <a href='posts/new'>Post to instagram</a>

	 <% if @posts.any? %>
	 <% @posts.each do |post| %>
	 <h2> <%= post.caption %> </h2>
	 <h2> <%= image_tag post.image.url(:medium) %> </h2>
	 <%= link_to "Edit post", edit_post_path(post) %>
	 <%= link_to "Delete post", post_path(post), method: :delete %>
	 <% end %>
	 <% else %>
	 No posts added yet
	 <a href='#'>Add a post</a>
	 <% end %>
